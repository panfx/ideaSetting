<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B' or 'Ctrl+Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Hide All Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'Ctrl+Shift+F12'</ShortCut>
          <Description>Hide All Windows</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Debug 'provisioning'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'Shift+F9'</ShortCut>
          <Description>Debug 'provisioning'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project Structure...">
        <Item>
          <IdeaActionID>ShowProjectStructureSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+S'</ShortCut>
          <Description>Project Structure...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'web'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'Ctrl+F5'</ShortCut>
          <Description>Rerun 'web'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'web'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Ctrl+F2'</ShortCut>
          <Description>Stop 'web'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop...">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Ctrl+F2'</ShortCut>
          <Description>Stop...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>